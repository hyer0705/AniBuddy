SELECT * FROM expert_file;

SELECT
    u.user_id, u.nick, u.is_expert, p.*
FROM user_tb u, expert_post p 
WHERE u.user_no = p.user_no
AND post_no = 241
ORDER BY post_no DESC;


-- commit;

-- 전문가 게시판 게시글, 댓글, 파일 테이블 select 문
SELECT * FROM free_post
ORDER BY post_no;

SELECT * FROM expert_post
ORDER BY post_no;

SELECT * FROM expert_comment;

SELECT * FROM free_comment;
